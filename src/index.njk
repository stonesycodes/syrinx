<h2>Vintage Computer Catalog</h2>

<ul class="catalog">
{% for machine in machines %}
  <li class="card" data-slug="{{ machine.slug }}">
    <h3>{{ machine.name }}</h3>
    <p><strong>Year:</strong> {{ machine.year }}</p>
    <p><strong>Specs:</strong> {{ machine.specs }}</p>

    <p class="status">Status: Loadingâ€¦</p>

    <a href="{{('/machines/' + machine.slug + '/') |url }}">View details</a>
  </li>
{% endfor %}
</ul>

